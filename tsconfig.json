{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": true,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "locale": "en",
    "strict": false,
    "watch": true,
    "removeComments": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "noEmitOnError": false,
    "paths": {
      "*": ["node_modules/*", "src/types/*"],
      "eldeeb": ["projects/eldeeb"],
      "eldeeb/*": ["projects/eldeeb/src/lib/*", "../projects/eldeeb/src/lib/*"],
      "config": ["config.js", "../config.js"],
      "jszip": ["node_modules/jszip/dist/jszip.min.js"]
    }
  }
}
