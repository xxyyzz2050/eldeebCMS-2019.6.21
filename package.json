{
  "name": "eldeeb-cms",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run build:ssr  && npm run serve:ssr",
    "start:dev": "npm run build:ssr-dev && npm run serve:ssr",
    "build:ssr": "ng build --prod && ng run eldeebCMS:server:production && npm run compile:ssr",
    "build:ssr-dev": "ng build --prod=false && ng run eldeebCMS:server && npm run compile:ssr",
    "compile:ssr": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "nodemon dist/server --inspaect --open --watch",
    "serve": "ng serve --open --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc": "npx compodoc -p package.json",
    "android": "tns run android --bundle",
    "ios": "tns run ios --bundle",
    "mobile": "tns run --bundle",
    "preview": "tns preview --bundle",
    "deploy:github": "ng build --prod --output-path docs --base-href /eldeebCMS/",
    "deploy:firebase": "npm run build:ssr && npm run build:firebase  && firebase deploy  && npm run serve:ssr",
    "build:firebase": "npm --prefix ./functions run build --prod",
    "all": "npm run build:ssr && npm run deploy:github && firebase deploy && mobile && npm run serve:ssr",
    "check": "npx npm-check -u"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~8.1.0-next.1",
